	% Package for embedding HTML links %
	\usepackage{hyperref}

	% Package for drawing images and shapes %
	\usepackage{tikz}

	% Package for putting multiple rows and multiple columns in tables %
	\usepackage{multicol}
	
	% Creates a CLI notation
	\newcommand{\CLI}[1]{\colorbox{gray}{\color{white}#1}}
	
	% Creates an yellow highlighted background text box
	\newcommand{\Notice}[1]{\colorbox{yellow}{#1}}

% Package for embedding code into a document %
	\usepackage{listings}
